	 $(document).ready(function() {
        // Listen for form submission
        $("#addressForm").submit(function(event) {
            // Prevent the default form submission
            event.preventDefault();

            // Get the form data
            var formData = $(this).serialize();

            // Make an AJAX request to submit the form
            $.ajax({
                type: "POST",
                url: $(this).attr("action"),
                data: formData,
                success: function(response) {
                    // Toggle visibility of other buttons
                    $(".btn-secondary").toggle();

                    // Toggle the text of the selected button
                    var button = $("#toggleButton");
                    button.text(button.text() === 'Selected' ? 'Select' : 'Selected');

                    // Show the selected button
                    button.show();
                },
                error: function(error) {
                    console.log("Error submitting form: " + error);
                }
            });
        });
    });
