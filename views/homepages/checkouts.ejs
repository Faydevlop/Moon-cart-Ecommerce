async function orderConfirm() {
    // Check if an address is selected
    var selectedAddress = document.querySelector('input[name="selectedAddress"]:checked');
    if (!selectedAddress) {
        Swal.fire({
            title: "Error!",
            text: "Please select an address before placing the order.",
            icon: "error",
            confirmButtonClass: "btn btn-danger"
        });
        return;
    }

    // Check if a payment method is selected
    var selectedPaymentMethod = document.querySelector('input[name="paymentmethod"]:checked');
    if (!selectedPaymentMethod) {
        Swal.fire({
            title: "Error!",
            text: "Please select a payment method before placing the order.",
            icon: "error",
            confirmButtonClass: "btn btn-danger"
        });
        return;
    }

    const result = await Swal.mixin({
        customClass: {
            confirmButton: "btn btn-success",
            cancelButton: "btn btn-danger"
        },
        buttonsStyling: false
    }).fire({
        title: "Place Order?",
        text: "Please tap Confirm to Place Order",
        icon: "warning",
        showCancelButton: true,
        confirmButtonText: "Confirm Order!",
        cancelButtonText: "Cancel Order!",
        reverseButtons: true
    });

    if (result.isConfirmed) {
        try {
            const response = await fetch('/orderConfirmation', {
                method: 'post',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ addressId, PaymentMethod })
            });

            if (response.ok) {
                // Display the "Order placed !!" message after successful order confirmation
                if (PaymentMethod === 'cashondelivery') {
                    Swal.fire({
                        title: "Order placed !!",
                        text: "Your Order has been confirmed.",
                        icon: "success"
                    }).then(() => {
                        setTimeout(() => {
                            location.reload();
                        }, 500);
                        window.location.href = "/success";
                    });
                } else if (PaymentMethod === 'upi') {
                    const res = await response.json();
                    razorpayFunc(res);
                }
            } else {
                console.error('Failed to send POST request:', response.status);
            }
        } catch (error) {
            console.error('Error during POST request:', error);
        }
    } else if (result.dismiss === Swal.DismissReason.cancel) {
        Swal.fire({
            title: "Cancelled",
            text: "Order cancelled",
            icon: "error"
        });
    }
}    


function razorpayFunc(res){

	// alert(res)
	console.log(res)
	const options = {
            key: res.key_id, // Enter the Key ID generated from the Dashboard
            amount: res.order.amount,
            currency: res.order.currency,
            name: "Moon Cart",
            description: "Your Product ",
            image: 'LOGO',
            order_id: res.orderId,
			handler: async function (response) {
			console.log(response);

			const data = {
				orderCreationId: res.orderId,
				razorpayPaymentId: response.razorpay_payment_id,
				razorpayOrderId: response.razorpay_order_id,
				razorpaySignature: response.razorpay_signature,
			};
			console.log('/////////////////////////////////////////////////')
			console.log(data);

			try {
				const verifyResponse = await fetch('/rzrpay-verify', {
					method: 'POST',
					headers: {
						'Content-Type': 'application/json',
					},
					body: JSON.stringify({ data }),
				});

				const result = await verifyResponse.json();

				if (result.msg==='success') {
					Swal.fire({
						title: "Order placed !!",
						text: "Your Order has been confirmed.",
						icon: "success",
					}).then(() => {
						setTimeout(() => {
							location.reload();
						}, 500);
						window.location.href = "/success";
					});
				} else {
					alert("Some verification issue caused. Please try again after some time.");
				}
			} catch (error) {
				console.error("Error during verification:", error);
				alert("An error occurred during verification. Please try again later.");
			}
		},
					prefill: {
                name: "Soumya Dey",
                email: "SoumyaDey@example.com",
                contact: "9999999999",
            },
            notes: {
                address: "Soumya Dey Corporate Office",
            },
            theme: {
                color: "#000",
            },
        };

		const rzp = new Razorpay(options);
    rzp.open();
}














	
